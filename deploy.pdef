# -*- coding: utf-8 -*-

from panda3d.core import *

class arcns(p3d):
    require("panda3d","morepy","audio","tk")
    file("config.prc")
    dir("misc","misc")
    dir("models","models")
    config(display_name="arcns")
    excludeModule("mainscene")
    excludeModule("persoscene")
    excludeModule("gamescene")
    mainModule("main")
